@using Garage.Models;
@model Settings

@{
    Layout = null;
}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

<table class="table">
    <thead>
        <tr>
            <th scope="col">PhoneNumber</th>
            <th scope="col">FacebookUrl</th>
            <th scope="col">TwitterUrl</th>
            <th scope="col">GoogleSiteUrl</th>
            <th scope="col">PinterestUrl</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model?.PhoneNumber</td>
            <td>@Model?.FacebookUrl</td>
            <td>@Model?.TwitterUrl</td>
            <td>@Model?.GoogleSiteUrl</td>
            <td>@Model?.PinterestUrl</td>
        </tr>
    </tbody>
</table>
<a asp-action="Edit" asp-controller="Settings">
    <button type="button" class="btn btn-primary">Edit</button>
</a>